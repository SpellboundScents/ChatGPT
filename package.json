{
  "name": "chatgpt",
  "version": "1.1.0-rc.2",
  "scripts": {
    "version:sync": "node scripts/sync-version.mjs",
    "dev:fe": "vite",
    "build:fe": "tsc && vite build",
    "dev": "pnpm tauri dev",
    "build": "pnpm tauri build",
    "updater": "tr updater",
    "release": "tr release --git",
    "fix:conf": "tr override --json.tauri_updater_active=false",
    "fix:tray": "tr override --json.tauri_systemTray_iconPath=\"icons/tray-icon-light.png\" --json.tauri_systemTray_iconAsTemplate=false",
    "fix:tray:mac": "tr override --json.tauri_systemTray_iconPath=\"icons/tray-icon.png\" --json.tauri_systemTray_iconAsTemplate=true",
    "download": "node ./scripts/download.js",
    "tr": "tr",
    "tauri": "tauri",
    "tauri:build": "bash -lc \"export TAURI_PRIVATE_KEY=$(/usr/bin/tr -d '\\n' < src-tauri/private.key); export TAURI_KEY_PASSWORD=${TAURI_KEY_PASSWORD}; pnpm tauri build\"",
    "build:virtualizer": "esbuild src/injected/virtualizer.ts --bundle --format=iife --platform=browser --target=es2020 --outfile=src-tauri/injected/virtualizer.js",
    "watch:virtualizer": "esbuild src/injected/virtualizer.ts --bundle --format=iife --platform=browser --target=es2020 --outfile=src-tauri/injected/virtualizer.js --watch",
    "build:virtualizer:loader": "esbuild src/injected/virtualizer-loader.ts --bundle --format=iife --platform=browser --target=es2020 --outfile=src-tauri/injected/virtualizer-loader.js"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^2.8.0",
    "@tauri-release/cli": "^0.2.3",
    "@types/lodash": "^4.14.191",
    "@types/node": "^20.19.11",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/react-syntax-highlighter": "^15.5.6",
    "@types/uuid": "^9.0.0",
    "@vitejs/plugin-react": "^3",
    "esbuild": "^0.25.9",
    "sass": "^1.56.2",
    "typescript": "^5.9.2",
    "vite": "^4",
    "vite-tsconfig-paths": "^4.0.2"
  },
  "license": "MIT",
  "author": "chirv <inbox@spellboundscentswax.com>",
  "keywords": [
    "chatgpt",
    "app",
    "desktop",
    "tauri",
    "macos",
    "linux",
    "windows"
  ],
  "homepage": "https://github.com/SpellboundScents/ChatGPT",
  "bugs": "https://github.com/SpellboundScents/ChatGPT/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/SpellboundScents/ChatGPT"
  },
  "dependencies": {
    "@ant-design/icons": "^4.8.0",
    "@tauri-apps/api": "^2",
    "@tauri-apps/plugin-autostart": "^2.5.0",
    "@tauri-apps/plugin-dialog": "^2",
    "@tauri-apps/plugin-fs": "^2",
    "@tauri-apps/plugin-opener": "^2.4.0",
    "@tauri-apps/plugin-os": "^2.3.0",
    "@tauri-apps/plugin-process": "^2",
    "@tauri-apps/plugin-shell": "^2.3.0",
    "@tauri-apps/plugin-updater": "^2",
    "antd": "^5.1.0",
    "dayjs": "^1.11.7",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^8.0.4",
    "react-router-dom": "^6.4.5",
    "react-syntax-highlighter": "^15.5.0",
    "uuid": "^9.0.0"
  },
  "packageManager": "pnpm@10.15.1+sha512.34e538c329b5553014ca8e8f4535997f96180a1d0f614339357449935350d924e22f8614682191264ec33d1462ac21561aff97f6bb18065351c162c7e8f6de67"
}
